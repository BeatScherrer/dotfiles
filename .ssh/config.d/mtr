Host daimlerVM
  Hostname win-vm.mt
  User mtdefault
  KexAlgorithms diffie-hellman-group1-sha1

Host daimlerMS
  Hostname sgagalvefts1.degag.corpintra.net
  User pastera
  ProxyCommand ssh daimlerVM -W %h:%p
  #ProxyJump daimlerVM
  # The proxyjump command is not supported in openssh client 1:6.7p1-5 (in our schroot)
  LocalForward 5000 sgagawvefts1.degag.corpintra.net:3389
  LocalForward 5001 53.71.108.190:443
  LocalForward 5002 sgagawvefts1.degag.corpintra.net:443

