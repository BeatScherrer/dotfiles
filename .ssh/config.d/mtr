  ForwardX11 yes

Host bb1
  Hostname beebot-1.mt
  User mtr
  ForwardX11 yes
  LocalForward 5001 localhost:80

Host bb2
  Hostname beebot-2.mt
  User mtr
  ForwardX11 yes
  LocalForward 5002 localhost:80
  LocalForward 5000 localhost:5000

Host pi1
  ProxyJump bb1
  Hostname 192.168.123.80
  User pi
  RemoteForward 7777 raspbian.raspberrypi.org:80
  RemoteForward 7778 archive.raspberrypi.org:443
  RemoteForward 7779 mirror.init7.net:80
  ForwardX11 yes

Host pi2
  ProxyJump bb2
  Hostname 192.168.123.80
  User pi
  RemoteForward 7777 raspbian.raspberrypi.org:80
  RemoteForward 7778 archive.raspberrypi.org:443
  RemoteForward 7779 mirror.init7.net:80
  ForwardX11 yes

Host ms
  Hostname ms-12.mt
  User mtr

###################################
# KSA
###################################
Host ksaUbuntu
  Hostname 10.137.243.104
  User mtr

Host ksaMS
  Hostname 10.137.243.104
  Port 2122
  ProxyCommand ssh ksaUbuntu -W %h:%p
  RemoteForward 5000 nas3.mt:81
  User mtr

Host ksaR25
  Hostname 10.157.208.239
  ProxyCommand ssh ksaUbuntu -W %h:%p
  RemoteForward 5000 nas3.mt:81
  User mtr

Host MS4
  Hostname ms-12.mt
  #LocalForward 5000 10.137.243.105:80
  User mtr

Host ksaUbuntu_2
  Hostname 10.137.243.104
  ProxyCommand ssh mtr@ms-4.mt -W %h:%p
  #LocalForward 5000 10.137.243.105:80
  User mtr

# use the '..._2' for external access to ksa (i.e. from home)
Host ksaMS_2
  Hostname 10.137.243.104
  Port 2122
  ProxyCommand ssh mtr@ksaUbuntu_2 -W %h:%p
  #RemoteForward 5000 nas3.mt:81
  LocalForward 5000 10.137.243.105:80
  User mtr

Host ksaR25
  Hostname 10.157.208.239
  ProxyCommand ssh mtr@ksaUbuntu_2 -W %h:%p
  #RemoteForward 5000 nas3.mt:81
  LocalForward 5000 10.137.243.105:80
